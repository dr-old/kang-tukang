{
  "expo": {
    "name": "kang-tukang",
    "slug": "kang-tukang",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-palu.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "jsEngine": "jsc",
    "splash": {
      "image": "./assets/images/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#EABB48"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danni02.kangtukang"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Logo.png",
        "backgroundColor": "#EABB48"
      },
      "package": "com.danni02.kangtukang"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon-tukang.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4beca3a6-9665-451a-8db0-665e4645da30"
      }
    }
  }
}
